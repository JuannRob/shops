{
  "expo": {
    "name": "shops",
    "slug": "shops",
    "version": "1.0.0",
    "scheme": "shops",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/shop-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/shop-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adapt-icon.png",
        "backgroundColor": "#fff"
      }
    }
  }
}
